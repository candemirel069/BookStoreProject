@model int?

@if (Model.HasValue)
{
    for (int i = 1; i < 6; i++)
    {
        var cssStar = "";
        if (i <= Model)
        {
            cssStar = "star-checked";
        }
        else
        {
            cssStar = "star-normal";
        }
        <span class="fa fa-star checked @cssStar"></span>
    }
}

